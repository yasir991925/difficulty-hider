{
  "manifest_version": 2,

  "name": "Difficult Hider",
  "description": "Hides Difficult in LeetCode",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["popup.js"]
    }
  ]
}